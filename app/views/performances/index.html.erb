<p id="notice"><%= notice %></p>

<h1>Listing Performamces</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @performamces.each do |performamce| %>
      <tr>
        <td><%= link_to 'Show', performamce %></td>
        <td><%= link_to 'Edit', edit_performamce_path(performamce) %></td>
        <td><%= link_to 'Destroy', performamce, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Performance', new_performamce_path %>
