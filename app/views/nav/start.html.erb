<div class="rvt_container rvt-m-all-sm">
  <% if User.copyright_librarian?(User.current_username) %>
    <%= render partial: 'cl_start' %>
  <% else %>
    <%= render partial: 'cm_start' %>
  <% end %>
</div>
<script type="text/javascript" charset="utf-8">
    $(document).ready(function() {
        $('#avalon_items_table').tablesorter(
            {
                theme: 'bootstrap',
                sortList: [[0,0], [1,0]],
                headers: {
                   '.no_sort' : {
                        sorter: false
                }
            }
        });
    });
</script>




