<p id="notice"><%= notice %></p>

<h1>Listing Recording Contributors</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @recording_contributors.each do |recording_contributor| %>
      <tr>
        <td><%= link_to 'Show', recording_contributor %></td>
        <td><%= link_to 'Edit', edit_recording_contributor_path(recording_contributor) %></td>
        <td><%= link_to 'Destroy', recording_contributor, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Recording contributor', new_recording_contributor_path %>
